                $('a[aria-expanded=true]').attr('aria-expanded', 'false');
            });

            $('#sidebarCollapse3').on('click', function () {
                $('#sidebar').toggleClass('active3');
            });

            //Code to dismiss Nav 2 (Overlay Nav)
            $('#dismiss, .overlay').on('click', function () {
                $('#sidebar').removeClass('active2');
                $('.overlay').fadeOut();
            });
        });

