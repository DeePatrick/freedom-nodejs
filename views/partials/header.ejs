                            if (this.label) {
                                //Parent Items Created
                                var li = $("<li><a 'data-toggle='collapse' class= list-unstyled components'>" + this.label + "</a></li>");
                                li.appendTo(parent);
                                //Sub Items Created
                                if (this.items && this.items.length > 0) {
                                    var ul = $("<ul class ='collapse list-unstyled'><li><a>" + this.label + "</a></li></ul>");
                                    ul.appendTo(li);
                                    buildUL(ul, this.items);
                                }
                            }
                        });
                    }
                    var ul = $("<ul class = 'menuitems'></ul>");
                    $('#jqxMenu').empty();
                    ul.appendTo("#jqxMenu");
                    buildUL(ul, source2);
                    ("#jqxMenu").jqxTree({});
                })();
            });
        })();
                                //End select language item from dropdown
    </script>
</head>
